{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": "warn",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
